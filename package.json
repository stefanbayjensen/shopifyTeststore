        "es6-object-assign": "^1.1.0",
        "es6-string-polyfills": "^1.0.0",
        "eslint": "^5.16.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-prettier": "^3.1.0",
        "lazysizes": "^5.1.0",
        "object-fit-images": "^3.2.4",
        "prettier": "^1.18.2",
        "slater": "^1.5.2",
        "sscroll": "^1.1.2",
        "tiny-slider": "^2.9.2"
